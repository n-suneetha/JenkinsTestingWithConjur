pipeline {
    agent any
    stages {
        stage("run") {
             steps {
                script {
                    userVar = null
                    passVar = null
withCredentials([conjurSecretCredential(credentialsId: 'multibranch-secret-1', variable: 'CONJUR_SECRET')]) {                                 
                                 
                                 passVar = CONJUR_SECRET
                    }
             
                    echo "Username: ${passVar}"
                    
                }
            }
        }
    }
}
