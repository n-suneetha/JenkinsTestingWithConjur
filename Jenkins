pipeline {
    agent any
    stages {
        stage("run") {
             steps {
                script {
                    userVar = null
                    passVar = null
withCredentials([conjurSecretUsername(credentialsId: 'multibranch-secret-1', passwordVariable: 'CONJUR_SECRET', usernameVariable: 'USERNAME')]) {
                                 userVar = USERNAME
                                 passVar = CONJUR_SECRET
                    }
                    echo "Username: ${userVar}"
                    echo "Username: ${passVar}"
                    
                }
            }
        }
    }
}
